{
	"name": "icanboogie/bind-message-bus",
	"type": "library",
	"description": "Binds icanboogie/message-bus to ICanBoogie",
	"keywords": [
		"message",
		"bus"
	],
	"homepage": "https://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/bind-message-bus/issues",
		"source": "https://github.com/ICanBoogie/bind-message-bus"
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=8.1",
		"icanboogie/icanboogie": "^6.0",
		"icanboogie/message-bus": "^3.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.6",
		"phpunit/phpunit": "^9.5"
	},
	"autoload-dev": {
		"psr-4": {
			"Test\\ICanBoogie\\Binding\\MessageBus\\": "tests/lib"
		},
		"classmap": [
			"tests/Application.php"
		]
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	},
	"extra": {
		"icanboogie": {
			"config-path": "config"
		}
	}
}
